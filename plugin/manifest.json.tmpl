{
  "$schema": "https://bitcart.ai/schemas/plugin/1.3.0/plugin.schema.json",
  "name": "{{ .Name }}",
  "author": "{{ .Author }}",
  "version": "1.0.0",
  "description": "{{ .Description }}",
  "constraints": { "bitcart": "" },
  "installs": [
    {{ range $i,$el := .FinalTypes }}{ "type": "{{ $el.Type }}", "path": "{{ $el.Path }}" }{{ if not (IsLast $i (len $.FinalTypes)) }},{{ end }}
    {{ end }}
  ]
}
